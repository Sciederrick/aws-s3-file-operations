<div id="fileListing">
  <div id="fileListing-id">
    <form action="/api/list" method="GET">
      <h2><span class="myIcons">{{fontawesome-icon icon="list"}}</span>File Listing</h2>
      <input type="submit" value="list files" class="btn btn-list-files">
    </form>
  </div>
  <div id="fileListing-content">
    {{#if listExists}}
    <div class="list">
      {{#each bucketObjects}}
      <div class="list-item">
        <div class="file">{{this.Key}}</div>
        <div class="small">{{this.LastModified}}</div>
        <div class="small">{{this.Size}}</div>
        <div class="label">{{this.StorageClass}}</div>
        <div class="">
          <form action="/api/delete" method="POST">
            <input type="hidden" id="{{this.Key}}" name="fileName"  value="{{this.Key}}">
            <input type="submit" value="delete" class="btn">
          </form>
        </div>
        <div class="">
          <form action="/api/download" method="POST">
            <input type="hidden" id="{{this.Key}}" name="fileName"  value="{{this.Key}}">
            <input type="submit" value="download" class="btn btn-file-download">
          </form>
        </div>
      </div>
      {{/each}}
    </div>
    {{/if}}
  </div>
</div>
