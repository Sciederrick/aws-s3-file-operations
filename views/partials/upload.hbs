<div id="fileUpload">
  <div id="fileUpload-id">
    <h2><span class="myIcons">{{fontawesome-icon icon="upload"}}</span>File Upload</h2>
  </div>
  <div id="fileUpload-content">
    <form action="/api/upload" method="POST" enctype="multipart/form-data">
      <div class="wrapper">
        <div class="file-upload">
          <input type="file" name="file"/>
          {{fontawesome-icon icon="arrow-up"}}
        </div>
        <input type="submit" value="submit" class="btn btn-file-submit">
      </div>
    </form>
  </div>
</div>
